#include <cstdio>

const int N = 5100;

struct art{
	int x, y;
}a[N];

bool cmp(art a, art b){
	if(a.x == b.x) return a.y > b.y;
	else return a.x > b.x;
}
int n;
int cnt[5];

int main(){
	scanf("%d", &n);
	for(int i = 1; i <= n; i++){
		scanf("%1d", &a[i].x);
	}
	for(int i = 1; i <= n; i++){
		scanf("%1d", &a[i].y);
	}
	
	for(int i = 1; i <= n; i++){
		if(a[i].x == 1 && a[i].y == 1) cnt[1]++;
		else if(a[i].x == 0 && a[i].y == 0) cnt[2]++;
		else if(a[i].x == 0 && a[i].y == 1) cnt[3]++;
		else if(a[i].x == 1 && a[i].y == 0) cnt[4]++;
	}

	for(int i = 1; i <= cnt[1]; i++){
		for(int j = 1; j <= cnt[4]; j++){
			int less_a = (n >> 1 - j - i);
			int less_b = (n >> 1 - (cnt[4] - j) - (cnt[1] - i));
			int need = i - cnt[i]; 
			if(need == 0){
				
			}
		}
	}
}
